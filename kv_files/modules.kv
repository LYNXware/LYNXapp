

<SpinnerLeft>:
    FloatLayout:
        size: root.size
        pos: root.pos
        GUI_Spinner:
            size_hint: (0.15, 0.05)
            pos_hint: {'center_x': 0.1, 'center_y': 0.05}
            text: 'select device'
            on_press: self.values = root.get_left_devices()
            on_text:
                root.change_device(self.text)
                root.parent.parent.update_start_window()



